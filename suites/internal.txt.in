name: codecs
plugin: attachment
requires: device.driver == 'HDA Intel'
command: cat /proc/asound/card*/codec#*

name: cpuinfo
plugin: attachment
command: cat /proc/cpuinfo

name: dmidecode
plugin: attachment
user: root
command: dmidecode

name: lspci
plugin: attachment
command: lspci -vvnn

name: modprobe
plugin: attachment
command: find /etc/modprobe.* -name \*.conf | xargs cat

name: modules
plugin: attachment
command: cat /etc/modules

name: sysctl
plugin: attachment
command: find /etc/sysctl.* -name \*.conf | xargs cat

name: gcov.tgz
plugin: attachment
requires: package.name == 'lcov'
user: root
command: gcov_tarball
