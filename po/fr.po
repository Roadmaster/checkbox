#: ../jobs/local.txt.in:10
msgid "Autotest Suite tests"
msgstr "Tests de la Suite Autotest"

#. description
#: ../jobs/local.txt.in:17
msgid "Bluetooth tests"
msgstr "Tests Bluetooth"

