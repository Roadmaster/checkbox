name: phoronix
plugin: external
_description: Automated benchmark testing
requires:
 package.name == 'git-core'
 package.name == 'php5-cli'
 package.name == 'python-pexpect'
user: root
command: phoronix_suite | filter_templates --whitelist='name=encode-ogg' --whitelist='name=unigine-sanctuary' --whitelist='name=unigine-tropics' --whitelist='name=lightsmark' --whitelist='name=norsetto-shadow' --whitelist='name=compliance-acpi' --whitelist='name=compliance-ogl' --whitelist='name=compliance-sensors' | filter_packages
